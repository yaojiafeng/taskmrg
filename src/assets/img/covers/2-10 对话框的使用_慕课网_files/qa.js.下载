define("qacommon/qa",["qacommon/js/put_qa","juicer"],function(e){var r=e.EP,n={container:"#qa-list"},o={render:function(e){e=$.extend(n,e||{});var r=$("#qa-list-tpl").html(),t=$(e.container);t.html(juicer(r,e)),o.rewardShowHide()},rewardShowHide:function(){var e=null;$(window).on("scroll",function(){var r=$(".js-qa-reward");if(!r[0])return!1;var n=$(window).scrollTop()+500;clearTimeout(e),e=setTimeout(function(){$.each(r,function(e,o){$(o).removeClass("slow-show"),"undefined"!=typeof $(r[e+1]).offset()&&n>Number($(o).offset().top)-200&&n<Number($(r[e+1]).offset().top)-200&&$(o).addClass("slow-show")})},300)}),$(n.container).on("click",".js-qa-reward-close",function(){$(".js-qa-reward").remove()})}};return{render:o.render,PutQAInit:e.init,replacehtml:e.replacehtml,EP:r}});